{
	"require": {
		"laravel/framework": "4.1.*",
		"anahkiasen/former": "3.0.*",
		"goonwood/asldap-auth": "dev-master",
		"pda/pheanstalk": "2.0.*",
		"fideloper/proxy": "2.0.*",
		"laravelbook/ardent": "2.4.*"
	},
	"require-dev": {
        "barryvdh/laravel-debugbar": "1.*"
	},
	"autoload": {
		"psr-0": {
			"Swapshop": "app/"
		},
		"classmap": [
			"app/controllers",
			"app/database/migrations",
			"app/database/seeds",
			"app/tests/TestCase.php"
		]
	},
	"scripts": {
		"pre-update-cmd": [
			"php artisan clear-compiled"
		],
		"post-install-cmd": [
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan optimize"
		]
	},
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "dev"
}
